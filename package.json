{
  "name": "karma-parcel",
  "version": "0.0.0",
  "description": "Parcel tests to Karma",
  "main": "dist/karma-parcel-plugin.js",
  "scripts": {
    "clean": "rm -rf .cache dist",
    "build": "tsc -p .",
    "prepare": "npm run clean && npm run build",
    "test": "mocha -r ts-node/register src/**/*.Spec.ts",
    "watch": "npm run test -- -w --reporter dot --watch-extensions ts",
    "karma": "npm run build && karma start"
  },
  "author": "Georgios Valotasios",
  "devDependencies": {
    "@types/karma": "^3.0.0",
    "@types/log4js": "^2.3.5",
    "@types/mocha": "^5.2.5",
    "@types/parcel-bundler": "^1.10.1",
    "@types/sinon": "^5.0.5",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.10.3",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "files": [
    "dist"
  ]
}
